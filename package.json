{
	"name": "temple-registry",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"db:generate": "drizzle-kit generate",
		"db:migrate:production": "wrangler d1 migrations apply temple-registry --remote",
		"db:migrate": "wrangler d1 migrations apply temple-registry --local",
		"db:studio": "drizzle-kit studio --config=drizzle.config.local.ts",
		"db:studio:production": "drizzle-kit studio",
		"pages:build": "bunx @cloudflare/next-on-pages",
		"preview": "bun pages:build && wrangler pages dev",
		"deploy": "bun pages:build && wrangler pages deploy",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts"
	},
	"dependencies": {
		"@hookform/resolvers": "^4.1.2",
		"@radix-ui/react-checkbox": "^1.1.4",
		"@radix-ui/react-label": "^2.1.2",
		"@radix-ui/react-slot": "^1.1.2",
		"better-sqlite3": "^11.8.1",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"drizzle-kit": "^0.30.5",
		"drizzle-orm": "^0.40.0",
		"drizzle-zod": "^0.7.0",
		"lucide-react": "^0.477.0",
		"next": "15.1.6",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-hook-form": "^7.54.2",
		"tailwind-merge": "^3.0.2",
		"tailwindcss-animate": "^1.0.7",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@cloudflare/next-on-pages": "1",
		"@cloudflare/workers-types": "^4.20250224.0",
		"@eslint/eslintrc": "^3",
		"@types/node": "^20",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"eslint": "^9",
		"eslint-config-next": "15.1.6",
		"postcss": "^8",
		"tailwindcss": "^3.4.1",
		"typescript": "^5",
		"vercel": "^41.3.0",
		"wrangler": "^3.111.0"
	}
}